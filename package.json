{
   "name": "nextjs-blog",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "concurrently \"next dev\" \"babel -w ./src --out-dir dist --extensions .ts,.tsx\"",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "typeorm:build": "babel ./src --out-dir dist --extensions .ts,.tsx",
      "migration:run": "typeorm migration:run -d dist/data-source.js",
      "migration:revert": "typeorm migration:revert -d dist/data-source.js",
      "entity:create": "typeorm entity:create"
   },
   "dependencies": {
      "@babel/cli": "^7.22.5",
      "@types/node": "^20.3.2",
      "@types/react": "18.2.14",
      "@types/react-dom": "18.2.6",
      "autoprefixer": "10.4.14",
      "concurrently": "^8.2.0",
      "eslint": "8.43.0",
      "eslint-config-next": "13.4.7",
      "gray-matter": "^4.0.3",
      "marked": "^5.1.0",
      "next": "13.4.7",
      "pg": "^8.11.1",
      "postcss": "8.4.24",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "reflect-metadata": "^0.1.13",
      "tailwindcss": "3.3.2",
      "typeorm": "^0.3.17",
      "typescript": "5.1.6"
   },
   "devDependencies": {
      "@babel/plugin-proposal-decorators": "^7.22.5"
   }
}
